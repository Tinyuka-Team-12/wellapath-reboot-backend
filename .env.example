# Server
PORT=4000
NODE_ENV=development
LOG_LEVEL=info

# Database
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/wellapath?schema=public"
# If you used 5433 above, change the port accordingly

# --- Auth (Cognito) ---
AUTH_ENABLED=true
AUTH_DEV_BYPASS=true
COGNITO_REGION=us-east-1                 # <-- set to your region
COGNITO_USER_POOL_ID=us-east-1_UeQZIEewM         # <-- set your Pool Id
COGNITO_CLIENT_ID=rr10getkbgpp157c8v0pbqjlt   # <-- SPA app client id (audience)
# Derived in code: COGNITO_ISSUER = https://cognito-idp.${COGNITO_REGION}.amazonaws.com/${COGNITO_USER_POOL_ID}



# --- Geo / Nominatim ---
NOMINATIM_BASE=https://nominatim.openstreetmap.org
NOMINATIM_CONTACT=dev@wellapath.local     # set to a real contact email/domain
GEO_CACHE_TTL=5m                          # cache results (e.g., 5m, 1h)
GEO_RATE_PER_SECOND=1                     # respect Nominatim usage

APIMEDIC_BASE=https://sandbox-healthservice.priaid.ch
APIMEDIC_USER=<<your_user>>
APIMEDIC_KEY=<<your_key>>
APIMEDIC_LANG=en-gb
APIMEDIC_FORMAT=json
APIMEDIC_AUTH_BASE=https://sandbox-authservice.priaid.ch

INFERMEDICA_BASE=https://api.infermedica.com/v3
INFERMEDICA_APP_ID=<<from Infermedica developer portal>>
INFERMEDICA_APP_KEY=<<from developer portal>>

SYMPTOM_ANALYZER_PROVIDER=none
